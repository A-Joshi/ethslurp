echo "11. hex2Ascii 0x31313232"                | tee tests/test11.txt
hex2Ascii 0x31313232                           >>tests/test11.txt

echo "12. echo \"0x31313232\" | hex2Ascii -"   | tee tests/test12.txt
echo 0x31313232 | hex2Ascii -                  >>tests/test12.txt

echo "14. ascii2Hex 1122"                      | tee tests/test14.txt
ascii2Hex 1122                                 >>tests/test14.txt

echo "15. echo \"1122\" | ascii2Hex -"         | tee tests/test15.txt
echo 1122 | ascii2Hex -                        >>tests/test15.txt
